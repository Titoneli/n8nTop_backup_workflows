e(cnpj, '[^0-9]', '', 'g') AS cnpj,\n    logradouro,\n    numero,\n    complemento,\n    bairro,\n    cidade,\n    estado,\n    cep,\n    email,\n    website,\n    telefone,\n    situacao,\n    codigo_regional\n  FROM json_to_recordset($1::json) AS x(\n    nome text,\n    nome_fantasia text,\n    cnpj text,\n    logradouro text,\n    numero text,\n    complemento text,\n    bairro text,\n    cidade text,\n    estado text,\n    cep text,\n    email text,\n    website text,\n    telefone text,\n    situacao text,\n    codigo_regional text\n  )\n),\n\n-- üß© Etapa 1: Insere cada registro em cor_pessoa com numera√ß√£o\npessoa_insert AS (\n  INSERT INTO public.cor_pessoa (\n    nome_pessoa,\n    cpf_cnpj_pessoa,\n    dom_tpo_pessoa,\n    telefone_pessoa,\n    email_pessoa,\n    cod_regional_sga\n  )\n  SELECT\n    d.nome,\n    d.cnpj,\n    'JUR√çDICA',\n    regexp_replace(d.telefone, '[^0-9]', '', 'g'),\n    d.email,\n    NULLIF(d.codigo_regional, '')::bigint\n  FROM dados d\n  RETURNING id_pessoa\n),\n\n-- üß© Etapa 2: associa cada id_pessoa ao mesmo √≠ndice do JSON (1:1)\npessoa_com_rn AS (\n  SELECT\n    p.id_pessoa,\n    ROW_NUMBER() OVER () AS rn\n  FROM pessoa_insert p\n),\n\n-- üß© Etapa 3: Insere um registro correspondente em cor_empresa (v√≠nculo direto)\nempresa_insert AS (\n  INSERT INTO public.cor_empresa (\n    id_pessoa,\n    url_logo,\n    dom_tpo_empresa,\n    dom_sit_empresa,\n    cod_empresa\n  )\n  SELECT\n    pr.id_pessoa,\n    NULL AS url_logo,\n    'Regional' AS dom_tpo_empresa,\n    'ATIVO' AS dom_sit_empresa,\n    NULLIF(d.codigo_regional, '')::bigint AS cod_empresa\n  FROM dados d\n  JOIN pessoa_com_rn pr ON pr.rn = d.rn\n  RETURNING id_empresa, id_pessoa, cod_empresa\n)\n\nSELECT '‚úÖ Inseridos com sucesso' AS status,\n       COUNT(*) AS total_registros,\n       MIN(id_empresa) AS primeiro_id,\n       MAX(id_empresa) AS ultimo_id\nFROM empresa_insert;\n","options":{"queryReplacement":"={{ JSON.stringify($('buscaDados').item.json.body || []) }